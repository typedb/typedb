{
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/graknlabs/grakn.git"
  },
  "name": "grakn-workbase",
  "version": "0.0.1-SNAPSHOT",
  "description": "Grakn Workbase",
  "author": "Grakn Labs",
  "homepage": "https://grakn.ai",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "npm install grpc --runtime=electron --target=1.8.6",
    "test": "./node_modules/jest/bin/jest.js ./test/unit",
    "build-linux": "electron-builder --linux",
    "e2e": "npm run pack && ./node_modules/jest/bin/jest.js ./test/e2e --runInBand"
  },
  "build": {
    "productName": "Grakn Workbase",
    "appId": "ai.grakn.workbase",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons",
      "category": "Development",
      "target": "tar.gz"
    }
  },
  "//": [
    "We add explicitly 'grpc' to Workbase's dependencies so that 'grakn' will pick-up this 'grpc' version (instead of its own)",
    "And in the postinstall we update grpc to a version that works in Electron environment, instead of default node."
  ],
  "dependencies": {
    "@blueprintjs/core": "^3.3.0",
    "@blueprintjs/icons": "^3.1.0",
    "@blueprintjs/select": "^3.1.0",
    "@blueprintjs/table": "^3.1.1",
    "codemirror": "^5.38.0",
    "electron-store": "^1.3.0",
    "grakn": "1.2.8",
    "grpc": "^1.13.1",
    "hex-to-hsl": "^1.0.2",
    "image-data-uri": "^1.1.1",
    "jquery": "^3.2.1",
    "prismjs": "^1.5.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-transition-group": "^2.4.0",
    "underscore": "1.8",
    "vis": "^4.21.0",
    "vue": "^2.5.8",
    "vue-color": "^2.6.0",
    "vue-draggable-resizable": "^1.7.1",
    "vue-electron": "^1.0.6",
    "vue-multipane": "^0.9.5",
    "vue-router": "^2.5.3",
    "vue-toasted": "^1.1.19",
    "vuex": "^2.3.1",
    "winston": "3.0.0-rc1"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.19",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^1.1.3",
    "chalk": "^2.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^1.8.6",
    "electron-builder": "^20.24.5",
    "electron-debug": "^1.4.0",
    "electron-devtools-installer": "^2.2.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-vue": "^4.5.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^23.0.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.9.2",
    "sass-loader": "^6.0.6",
    "spectron": "^3.8.0",
    "style-loader": "^0.18.2",
    "url-loader": "^1.0.1",
    "vue-eslint-parser": "^1.0.0",
    "vue-html-loader": "^1.2.4",
    "vue-jest": "^2.6.0",
    "vue-loader": "^13.0.5",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.5.2",
    "webpack-dev-server": "^2.7.1",
    "webpack-hot-middleware": "^2.18.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.vue$": "<rootDir>/node_modules/vue-jest"
    },
    "moduleNameMapper": {
      "@/(.*)$": "<rootDir>/src/renderer/$1"
    }
  }
}
