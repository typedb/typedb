  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "^8.0",
    "babel-jest": "^21.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.12.0",
