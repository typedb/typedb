{
  "name": "grakn",
  "version": "1.2.5",
  "description": "Grakn Node.js Client",
  "main": "dist/Grakn.js",
  "author": "Grakn Labs",
  "license": "Apache-2.0",
  "homepage": "https://grakn.ai",
  "repository": {
    "type": "git",
    "url": "https://github.com/graknlabs/grakn"
  },
  "scripts": {
    "test": "./node_modules/jest/bin/jest.js",
    "build": "rm -rf dist/ && BABEL_ENV=production babel src -d dist",
    "compileSession": "grpc_tools_node_protoc --proto_path=../client-protocol/proto/ --js_out=import_style=commonjs,binary:./src/service/Session/autogenerated --grpc_out=./src/service/Session/autogenerated ../client-protocol/proto/{Session,Concept,Answer}.proto",
    "compileKeyspace": "grpc_tools_node_protoc --proto_path=../client-protocol/proto/ --js_out=import_style=commonjs,binary:./src/service/Keyspace/autogenerated --grpc_out=./src/service/Keyspace/autogenerated ../client-protocol/proto/Keyspace.proto",
    "compileGrpc": "mkdir ./src/service/Session/autogenerated && yarn run compileSession && mkdir ./src/service/Keyspace/autogenerated && yarn run compileKeyspace",
    "pretest": "yarn run compileGrpc",
    "preversion": "yarn run compileGrpc && yarn test && yarn build && TEST_ENV=dist yarn test",
    "version": "git add ."
  },
  "dependencies": {
    "google-protobuf": "^3.6.0",
    "grpc": "^1.12.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-minify": "^0.4.3",
    "grpc-tools":"^1.6.6",
    "jest": "^23.4.0"
  }
}